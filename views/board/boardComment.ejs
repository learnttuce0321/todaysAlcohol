<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1.0"
        />
        <title>Document</title>
    </head>
    <body>
        <section id="comments">
            <ul class="commentList_ul">
                <% comments.forEach((comment) => { %>
                <li>
                    <strong><%= comment.userId %></strong>
                    <p><%= comment.content %></p>
                </li>
                <% }); %>
            </ul>
        </section>
    </body>
</html>
